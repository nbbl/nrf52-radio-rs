{
  "version": "0.2.0",
  "configurations": [
    {
      "preLaunchTask": "Build Hello World",
      "type": "probe-rs-debug",
      "request": "launch",
      "name": "Run with probe-rs",
      "flashingConfig": {
        "flashingEnabled": true,
      },
      "chip": "nRF52840_xxAA",
      "coreConfigs": [
        {
          // Change this to the binary you want to debug
          "programBinary": "${workspaceFolder}/target/thumbv7em-none-eabihf/debug/hello",
          "rttEnabled": true
        }
      ],
    }
  ]
}